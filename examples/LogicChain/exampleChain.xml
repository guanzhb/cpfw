<?xml version="1.0" encoding="UTF-8"?>
<exampleChain>

    <profiles>
        <profile widget="volume" id="11221" bindTo="">
            <element name="default" id="0" min="1" max="50" current="5" type="3">default</element>
        </profile>
        <profile widget="fade" id="11222" bindTo="">
            <element name="default0" id="0"  min="1" max="20" current="5" type="3">fade0</element>
            <element name="default1" id="1"  min="1" max="20" current="5" type="3">fade1</element>
            <element name="default2" id="2"  min="1" max="20" current="5" type="3">fade2</element>
            <element name="default3" id="3"  min="1" max="20" current="5" type="3">fade3</element>
            <element name="default4" id="4"  min="1" max="20" current="5" type="3">fade4</element>
            <element name="default5" id="5"  min="1" max="20" current="5" type="3">fade5</element>
            <element name="default6" id="6"  min="1" max="20" current="5" type="3">fade6</element>
        </profile>
        <profile widget="equalizer" id="11223" bindTo="">
            <element name="gain_100hz" id="0"  min="1" max="20" current="5" type="1"></element>
            <element name="gain_200hz" id="1"  min="5" max="20" current="10" type="1"></element>
            <element name="gain_400hz" id="2"  min="20" max="30" current="5" type="1"></element>
            <element name="gain_800hz" id="3"  min="50" max="200" current="100" type="1"></element>
        </profile>
        <profile widget="duck" id="11224" bindTo="11221">
            <element name="default" id="0"  min="0" max="100" current="80" type="3">default</element>
        </profile>
        <profile widget="loudness" id="11225" bindTo="">
            <element name="default" id="0"  min="10" max="20" current="5" type="1">default</element>
            <element name="default1" id="1"  min="10" max="20" current="5" type="1">default1</element>
        </profile>
        <profile widget="stub" id="11226" bindTo="">
            <element name="default" id="0"  min="10" max="20" current="5" type="1">default</element>
        </profile>
        <profile widget="loop" id="11227" bindTo="">
            <element name="default" id="0"  min="10" max="80" current="5" type="1">default</element>
        </profile>
    </profiles>

    <invokeChains>
        <parent widget="volume">
            <child widget="loudness"></child>
            <child widget="duck"></child>
        </parent>
    </invokeChains>

    <conditions>
        <condition widget="loudness" logic="and">
            <element when="default@volume;equal:20"></element>
            <element when="default@fade;equal:5"></element>
            <element when="gain_400hz@equalizer;equal:5"></element>
        </condition>
        <condition widget="duck" logic="or">
            <element when="default@volume;in_range:1,200"></element>
        </condition>
        <condition widget="equalizer" logic="or">
            <element when="gain_100hz@equalizer;change"></element>
            <element when="gain_200hz@equalizer;change"></element>
            <element when="gain_400hz@equalizer;change"></element>
            <element when="gain_800hz@equalizer;change"></element>
        </condition>
    </conditions>

    <converts>
        <context widget="duck"
            calculate="mul_variable:default@volume;div_const:100">
        </context>
        <context widget="fade" calculate="mul_const:20;sub_variable:gain_400hz@equalizer;div_const:2">
            <element origin="1" target="22222"></element>
            <element origin="2" target="3333"></element>
        </context>
    </converts>
</exampleChain>
